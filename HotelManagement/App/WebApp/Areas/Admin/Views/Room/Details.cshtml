@using Microsoft.AspNetCore.Mvc.TagHelpers
@model App.Domain.Room

@{
    ViewData["Title"] = "Details";
}

<h1>@App.Resources.WebApp.Views.Shared.CRUD.Details</h1>

<div>
    <h4>Room</h4>
    <hr />

    <partial name="_Details" model="Model"></partial>

</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">@App.Resources.WebApp.Views.Shared.CRUD.Edit</a> |
    <a asp-action="Index">@App.Resources.WebApp.Views.Shared.CRUD.BackToList</a>
</div>
